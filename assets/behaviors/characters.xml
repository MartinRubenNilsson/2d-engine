<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="enemy">
    <Delay delay_msec="2000">
      <Sequence>
        <ConsoleLog message="I&apos;m outta here!"/>
        <DestroySelf/>
      </Sequence>
    </Delay>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="ConsoleLog">
      <input_port name="message"
                  type="std::string">test</input_port>
    </Action>
    <Action ID="DestroySelf"/>
  </TreeNodesModel>

</root>
