<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="enemy">
    <Delay delay_msec="2000">
      <ConsoleExecute command="map close"/>
    </Delay>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="ConsoleExecute">
      <input_port name="command"
                  type="std::string">The command line to execute</input_port>
    </Action>
  </TreeNodesModel>

</root>
